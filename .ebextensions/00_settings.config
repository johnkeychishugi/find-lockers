option_settings:
  "aws:elasticbeanstalk:container:php:phpini":
    document_root: /public
    memory_limit: 256M
    zlib.output_compression: "Off"
    allow_url_fopen: "On"
    display_errors: "On"
    max_execution_time: 90
# uncomment to use apache as proxy server, 
# but I faced issues with this so I don't recommend
#  "aws:elasticbeanstalk:environment:proxy":
#    ProxyServer: apache
  "aws:elasticbeanstalk:application:environment":
    APP_ENV: staging
    APP_URL: http://yourelasticbeanstalk.elasticbeanstalk.com
    APP_DOMAIN: yourelasticbeanstalk.elasticbeanstalk.com
    DB_HOST: rdshostname.rds.amazonaws.com
    DB_DATABASE: ebdb
    DB_USERNAME: yourdbuser
    DB_PASSWORD: "yourdbpass"
    APP_NAME: TheCodeDecode

    APP_KEY: "base64:sICADOp95FcSLc7bfb8Uyh34jMDFxE9dOvY8RxBIKlQ="
    APP_DEBUG: true
    LOG_CHANNEL: stack

    BROADCAST_DRIVER: log
    CACHE_DRIVER: file
    QUEUE_CONNECTION: sync
    SESSION_DRIVER: file
    SESSION_LIFETIME: 120

    REDIS_HOST: 127.0.0.1
    REDIS_PASSWORD: ""
    REDIS_PORT: 6379

    MAIL_MAILER: smtp
    MAIL_HOST: smtp.mailtrap.io
    MAIL_PORT: 2525
    MAIL_USERNAME: ""
    MAIL_PASSWORD: ""
    MAIL_ENCRYPTION: ""
    MAIL_FROM_ADDRESS: ""
    MAIL_FROM_NAME: "${APP_NAME}"

    AWS_ACCESS_KEY_ID: ""
    AWS_SECRET_ACCESS_KEY: ""
    AWS_DEFAULT_REGION: eu-central-1
    AWS_BUCKET: ""

    PUSHER_APP_ID: ""
    PUSHER_APP_KEY: ""
    PUSHER_APP_SECRET: ""
    PUSHER_APP_CLUSTER: mt1

    MIX_PUSHER_APP_KEY: "${PUSHER_APP_KEY}"
    MIX_PUSHER_APP_CLUSTER: "${PUSHER_APP_CLUSTER}"

